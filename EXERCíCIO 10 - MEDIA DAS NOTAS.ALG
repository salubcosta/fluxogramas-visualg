Algoritmo "Exercício 10 - Média das notas"
Var
   N1, N2, N3, M: real
Inicio
      escreva("Informe nota 1: ")
      leia(N1)
      escreva("Informe nota 2: ")
      leia(N2)
      escreva("Informe nota 3: ")
      leia(N3)

      M <- (N1 + N2 + N3) / 3

      escreva("A média das notas é: ", M)
Fimalgoritmo