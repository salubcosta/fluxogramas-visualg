Algoritmo "Exercício 22 - Quociente e Resto"
Var
   a, b, q, r: inteiro
Inicio
      escreva("Informe o valor de A: ")
      leia(a)
      
      escreva("Informe o valor de B: ")
      leia(b)
      
      q <- a div b
      
      r <- a mod b
      
      escreval("Quociente: ", q)
      escreva("Resto: ", r)
Fimalgoritmo